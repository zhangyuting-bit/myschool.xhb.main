<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.ExpressionMapper">
    <select id="getExpressionAll" resultType="com.zb.entity.Expression">
        SELECT
            *
        FROM
            `expression`
    </select>

    <select id="getExpressionById" resultType="com.zb.entity.Expression">
        SELECT
            *
        FROM
            `expression`
        WHERE
            expressionId=#{expressionId}
    </select>
</mapper>