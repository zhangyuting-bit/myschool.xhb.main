<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.MudelMapper">
    <select id="getMudelByTypeId" resultType="com.zb.entity.Mudel">
        SELECT
            *
        FROM
            `mudel`
        WHERE
            typeId=#{typeId}
    </select>
    
    <select id="getMudelById" resultType="com.zb.entity.Mudel">
        SELECT
            *
        FROM
            `mudel`
        WHERE
            mudelId=#{mudelId}
    </select>
</mapper>