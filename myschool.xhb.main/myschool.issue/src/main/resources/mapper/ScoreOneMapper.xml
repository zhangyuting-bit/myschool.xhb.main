<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.ScoreOneMapper">
    <insert id="addScoreOne">
        INSERT INTO `issue`.`scoreone`
            (`oneId`,
             `scoreId`,
             `userId`)
        VALUES (#{oneId},
                #{scoreId},
                #{userId});
    </insert>
    
    <delete id="delScoreOneByScoreId">
        delete from scoreone where scoreId=#{scoreId}
    </delete>
</mapper>