<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.AddExpressionMapper">
    <select id="getAddExpressionByNId" resultType="com.zb.entity.AddExpression">
        SELECT
            *
        FROM
            `addexpression`
        WHERE
            functionId=#{functionId}
    </select>

    <insert id="addAddExpression">
        INSERT INTO `addexpression`
            (`id`,
             `expressionId`,
             `functionId`)
        VALUES (#{id},
                #{expressionId},
                #{functionId});
    </insert>
</mapper>